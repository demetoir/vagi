{
	"name": "backend",
	"version": "1.0.0",
	"engines": {
		"node": ">=12.16.3"
	},
	"bin": {
		"mocha": "./node_modules/.bin/mocha",
		"nodemon": "./node_modules/.bin/nodemon"
	},
	"scripts": {
		"node:babel": "node -r @babel/register",
		"reinstall": "rm -rf ../node_modules && yarn install",
		"test": "cross-env NODE_ENV=test mocha ./spec -name '*.spec.js' --recursive --exit",
		"test:watch": "cross-env NODE_ENV=test mocha ./spec --recursive --watch",
		"test:only": "cross-env NODE_ENV=test mocha --watch",
		"build": "node script/build.js",
		"build:babel": "node script/build-babel.js",
		"build:inject-env": "node script/build-inject-env.js",
		"build:clean": "rm -rf ./build",
		"start": "docker-compose -f docker/docker-development.yml up",
		"start:express": "cross-env NODE_ENV=development nodemon --config ./nodemon.express.json --exec babel-node ./express/index.js",
		"start:socket": "cross-env NODE_ENV=development nodemon --config ./nodemon.socket.json --exec babel-node ./socket_io_server/app.js",
		"start:yoga": "cross-env NODE_ENV=development nodemon --config ./nodemon.graphql.json --exec babel-node ./graphQL/app.js",
		"stop": "docker-compose -f docker/docker-development.yml down",
		"migration:development": "sh ./script/sequelize-migration-dev.sh",
		"migration:production": "sh ./script/sequelize-migration-product.sh",
		"lint": "npx eslint .",
		"lint:fix": "npx eslint --fix .",
		"pretty": "sh script/pretty.sh",
		"docker:build": "sh ./script/build-docker-image.sh",
		"docker:build:force": "rm -rf docker/cached && sh ./script/build-docker-image.sh",
		"docker:volumes:remove": "rm -rf docker/volumes",
		"deploy": "docker-compose -f docker/docker-product.yml up -d",
		"deploy:watch": "docker-compose -f docker/docker-product.yml up",
		"deploy:express:monitor": "docker exec -it express yarn pm2 monit",
		"deploy:undo": "docker-compose -f docker/docker-product.yml down"
	},
	"main": "index.js",
	"license": "MIT",
	"dependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "^7.5.5",
		"@babel/node": "^7.8.7",
		"@babel/plugin-transform-runtime": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/register": "^7.5.5",
		"@babel/runtime": "^7.5.5",
		"@types/chai": "^4.2.11",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.2",
		"@types/faker": "^4.1.9",
		"@types/glob": "^7.1.1",
		"@types/jsonwebtoken": "^8.3.8",
		"@types/node": "^13.7.1",
		"@types/passport": "^1.0.2",
		"@types/passport-google-oauth20": "^2.0.3",
		"@types/passport-jwt": "^3.0.3",
		"@types/sequelize": "^4.28.8",
		"@types/shelljs": "^0.8.7",
		"@types/sinon": "^9.0.0",
		"@types/supertest": "^2.0.8",
		"@types/url-parse": "^1.4.3",
		"async-redis": "^1.1.7",
		"babel-watch": "^7.0.0",
		"btoa": "^1.2.1",
		"chai": "^4.2.0",
		"chalk": "^4.0.0",
		"cli-progress": "^3.6.0",
		"concurrently": "^5.0.0",
		"cookie-parser": "^1.4.4",
		"cors": "^2.8.5",
		"cross-env": "^7.0.2",
		"date-utils": "^1.2.21",
		"dotenv": "^8.2.0",
		"easygraphql-tester": "^5.1.6",
		"eslint": "^6.0.1",
		"eslint-config-naver": "^2.1.0",
		"eslint-plugin-import": "^2.18.2",
		"express": "^4.17.1",
		"faker": "^4.1.0",
		"glob": "^7.1.6",
		"graphql-tag": "^2.10.3",
		"graphql-yoga": "^1.18.3",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.19",
		"merge-graphql-schemas": "^1.7.3",
		"mocha": "^7.1.1",
		"moment": "^2.29.2",
		"morgan": "^1.9.1",
		"mysql2": "^2.0.0",
		"node-mocks-http": "^1.8.0",
		"nodemon": "^2.0.2",
		"passport": "^0.4.0",
		"passport-google-oauth20": "^2.0.0",
		"passport-jwt": "^4.0.0",
		"passport-mocked": "^1.4.0",
		"pm2": "^4.1.2",
