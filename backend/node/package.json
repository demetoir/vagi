		"redis": "^3.0.2",
		"sequelize": "^5.21.2",
		"sequelize-cli": "^5.5.1",
		"shelljs": "^0.8.3",
		"should": "^13.2.3",
		"sinon": "^9.0.1",
		"socket.io": "^2.3.0",
		"sqlite3": "^4.1.1",
		"supertest": "^4.0.2",
		"url-parse": "^1.4.7",
		"uuid": "^7.0.3",
		"winston": "^3.2.1",
		"winston-daily-rotate-file": "^4.3.0"
	},
	"devDependencies": {}
}
